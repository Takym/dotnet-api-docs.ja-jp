    <altmember cref="T:System.Web.Configuration.AssemblyCollection" />
    <altmember cref="T:System.Configuration.ConfigurationElement" />
    <related type="Article" href="https://msdn.microsoft.com/library/9d6166a6-d69f-4330-89f8-64c597221951">ASP.NET コンパイルモデル</related>
